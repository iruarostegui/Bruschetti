e":"js/contact.js","original":{"line":49,"column":4}},{"name":"$","generated":{"line":54,"column":4},"source":"js/contact.js","original":{"line":49,"column":4}},{"generated":{"line":54,"column":5},"source":"js/contact.js","original":{"line":49,"column":5}},{"generated":{"line":54,"column":6},"source":"js/contact.js","original":{"line":49,"column":6}},{"generated":{"line":54,"column":15},"source":"js/contact.js","original":{"line":49,"column":5}},{"generated":{"line":54,"column":16},"source":"js/contact.js","original":{"line":49,"column":4}},{"name":"slideUp","generated":{"line":54,"column":17},"source":"js/contact.js","original":{"line":49,"column":17}},{"generated":{"line":54,"column":24},"source":"js/contact.js","original":{"line":49,"column":4}},{"generated":{"line":55,"column":0},"source":"js/contact.js","original":{"line":50,"column":3}},{"generated":{"line":55,"column":3},"source":"js/contact.js","original":{"line":47,"column":2}},{"generated":{"line":56,"column":0},"source":"js/contact.js","original":{"line":51,"column":1}},{"generated":{"line":56,"column":1},"source":"js/contact.js","original":{"line":1,"column":0}}],"sources":{"js/contact.js":"$(document).ready(function() {\n  $(\"#submit_btn\").click(function() {\n\n    var proceed = true;\n    //simple validation at client's end\n    //loop through each field and we simply change border color to red for invalid fields       \n    $(\"#contact_form input[required=true], #contact_form textarea[required=true]\").each(function() {\n      $(this).css('border-color', '');\n      if (!$.trim($(this).val())) { //if this field is empty \n        $(this).css('border-color', 'red'); //change border color to red   \n        proceed = false; //set do not proceed flag\n      }\n      //check invalid email\n      var email_reg = /^([\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4})?$/;\n      if ($(this).attr(\"type\") == \"email\" && !email_reg.test($.trim($(this).val()))) {\n        $(this).css('border-color', 'red'); //change border color to red   \n        proceed = false; //set do not proceed flag              \n      }\n    });\n\n    if (proceed) //everything looks good! proceed...\n    {\n      //get input field values data to be sent to server\n      post_data = {\n        'user_name': $('input[name=name]').val(),\n        'user_email': $('input[name=email]').val(),\n        'phone_number': $('input[name=phone2]').val(),\n        'msg': $('textarea[name=message]').val()\n      };\n\n      //Ajax post data to server\n      $.post('php/contact_me.php', post_data, function(response) {\n        if (response.type == 'error') { //load json data from server and output message     \n          output = '<div class=\"error\">' + response.text + '</div>';\n        } else {\n          output = '<div class=\"success\">' + response.text + '</div>';\n          //reset values in all input fields\n          $(\"#contact_form  input[required=true], #contact_form textarea[required=true]\").val('');\n          $(\"#contact_form #contact_body\").slideUp(); //hide form after success\n        }\n        $(\"#contact_form #contact_results\").hide().html(output).slideDown();\n      }, 'json');\n    }\n  });\n\n  //reset previously set border colors and hide all message on .keyup()\n  $(\"#contact_form  input[required=true], #contact_form textarea[required=true]\").keyup(function() {\n    $(this).css('border-color', '');\n    $(\"#result\").slideUp();\n  });\n});"},"lineCount":null}},"error":null,"hash":"796c96ed8b92d1a309f0d1ed87f3648e","cacheData":{"env":{}}}